{
    "name": "w-cms/w-cms",
    "description": "point'n think",
    "require": {
        "php": ">=7.4.0",
        "altorouter/altorouter": "^1.2",
        "firebase/php-jwt": "^5.2",
        "jamesmoss/flywheel": "^0.5.2",
        "league/plates": "^3.3",
        "michelf/php-markdown": "^1.8"
    },
    "require-dev": {
        "filp/whoops": "^2.7",
        "pepakriz/phpstan-exception-rules": "^0.10.1",
        "phpstan/phpstan": "^0.12.19",
        "phpstan/phpstan-phpunit": "^0.12.8",
        "phpunit/phpunit": "^8.5",
        "sentry/sdk": "^2.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Wcms\\": "app/class"
        },
        "files": ["app/fn/fn.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Wcms\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
